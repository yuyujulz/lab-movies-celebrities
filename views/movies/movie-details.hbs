<h1>{{title}}</h1>
<p> Genre: {{genre}} </p>
<p> Plot: {{plot}} </p>
<h1>Cast:</h1>
{{#each cast}}
   <p>{{name}}</p>
   <p>Occupation: {{occupation}}</p>
    <p>Favorite catchphrase: {{catchPhrase}}</p> 
{{/each}}

<form action="/movies/{{_id}}/delete" method="POST">
<button class="submitBtn" type="submit">Delete </button>
</form>

<a class="backHomeLink" href="/movies">Back</a>